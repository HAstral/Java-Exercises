package samp;
import java.util.*;
public class no7 {
	public static void main(String []args) {
	Scanner scanner=new Scanner(System.in);
	System.out.print("Enter a number:");
	int num=scanner.nextInt();
	while(num>0)
	
	{
		int i=2;
		if(num<=i && num%num==0)
			{System.out.println("Prime");}
		else {
			while(i<num)
			{ if(num%i==0)
				System.out.println("not Prime");
				
			}
			i++;
	}
	}
}
}	
